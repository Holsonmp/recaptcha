{
    "name": "holduix/recaptcha",
    "description": "Google reCAPTCHA integration for PHP (supports v2 and v3)",
    "keywords": ["recaptcha", "captcha", "google", "v2", "v3", "security", "spam", "bot", "protection", "CodeIgniter"],
    "homepage": "https://github.com/holsonmp/recaptcha",
    "authors": [
        {
            "name": "Holsonmp",
            "homepage": "https://github.com/holsonmp",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/holsonmp/recaptcha/issues",
        "source": "https://github.com/holsonmp/recaptcha/tree/master",
        "docs": "https://github.com/holsonmp/recaptcha#readme"
    },
    "license": "GPL-2.0-only",
    "version": "2.0.0",
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Holduix\\Component\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Holduix\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs",
        "fix-style": "phpcbf src tests"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}